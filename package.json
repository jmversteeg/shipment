    "lodash": "^4.17.4",
    "morgan": "^1.9.0",
    "sprintf-js": "^1.1.1",
    "stream-line-wrapper": "^0.1.1",
    "through2": "^2.0.3",
    "uuid": "^3.2.1",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.16.0",
    "execa": "^0.9.0",
    "istanbul": "^0.4.5",
