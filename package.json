{
  "name": "shipment",
  "version": "11.0.0-1",
  "description": "Run asynchronous actions and report realtime events through auto-generated CLI, node module API, or HTTP API.",
  "main": "lib/Shipment.js",
  "engines": {
    "node": ">=7.6"
  },
  "scripts": {
    "pretest": "eslint lib ; exit 0",
    "test": "mocha --recursive -R spec \"./test/{,!(support)/**/}*.js\"",
    "test-coverage": "istanbul cover ./node_modules/mocha/bin/_mocha -- --recursive \"./test/{,!(support)/**/}*.js\" && opn coverage/lcov-report/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/launchdeckio/shipment.git"
  },
  "keywords": [
    "cli",
    "api",
    "subcommands",
    "subcommand"
  ],
  "author": "JM Versteeg",
  "license": "MIT",
  "dependencies": {
